{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "codigoMeio",
    "codigoPreferencia",
    "codigoUso",
    "detalhe"
  ],
  "properties": {
    "codigoMeio": {
      "$id": "#/properties/codigoMeio",
      "type": "integer",
      "title": "The Codigomeio Schema",
      "default": 0,
      "examples": [
        1234
      ]
    },
    "codigoPreferencia": {
      "$id": "#/properties/codigoPreferencia",
      "type": "string",
      "title": "The Codigopreferencia Schema",
      "default": "",
      "examples": [
        "A"
      ],
      "pattern": "^(.*)$"
    },
    "codigoUso": {
      "$id": "#/properties/codigoUso",
      "type": "integer",
      "title": "The Codigouso Schema",
      "default": 0,
      "examples": [
        1234
      ]
    },
    "detalhe": {
      "$id": "#/properties/detalhe",
      "type": "string",
      "title": "The Detalhe Schema",
      "default": "",
      "examples": [
        "..."
      ],
      "pattern": "^(.*)$"
    }
  }
}