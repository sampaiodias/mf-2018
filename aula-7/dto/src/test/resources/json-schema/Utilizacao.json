{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "idIdentificador",
    "codigoUso",
    "dataInicial",
    "dataFinal"
  ],
  "properties": {
    "idIdentificador": {
      "$id": "#/properties/idIdentificador",
      "type": "integer",
      "title": "The Ididentificador Schema",
      "default": 0,
      "examples": [
        1234
      ]
    },
    "codigoUso": {
      "$id": "#/properties/codigoUso",
      "type": "integer",
      "title": "The Codigouso Schema",
      "default": 0,
      "examples": [
        123
      ]
    },
    "dataInicial": {
      "$id": "#/properties/dataInicial",
      "type": "string",
      "title": "The Datainicial Schema",
      "default": "",
      "examples": [
        "20181114"
      ],
      "pattern": "^(.*)$"
    },
    "dataFinal": {
      "$id": "#/properties/dataFinal",
      "type": "string",
      "title": "The Datafinal Schema",
      "default": "",
      "examples": [
        "20181114"
      ],
      "pattern": "^(.*)$"
    }
  }
}