{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "serie",
    "codigoEstado"
  ],
  "properties": {
    "serie": {
      "$id": "#/properties/serie",
      "type": "integer",
      "title": "The Serie Schema",
      "default": 0,
      "examples": [
        111
      ]
    },
    "codigoEstado": {
      "$id": "#/properties/codigoEstado",
      "type": "string",
      "title": "The Codigoestado Schema",
      "default": "",
      "examples": [
        "GO"
      ],
      "pattern": "^(.*)$"
    }
  }
}