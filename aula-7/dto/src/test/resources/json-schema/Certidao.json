{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "codigoTipo",
    "cartorio",
    "livro",
    "folha",
    "termo"
  ],
  "properties": {
    "codigoTipo": {
      "$id": "#/properties/codigoTipo",
      "type": "integer",
      "title": "The Codigotipo Schema",
      "default": 0,
      "examples": [
        1234
      ]
    },
    "cartorio": {
      "$id": "#/properties/cartorio",
      "type": "string",
      "title": "The Cartorio Schema",
      "default": "",
      "examples": [
        "Cart√≥rio A"
      ],
      "pattern": "^(.*)$"
    },
    "livro": {
      "$id": "#/properties/livro",
      "type": "integer",
      "title": "The Livro Schema",
      "default": 0,
      "examples": [
        2
      ]
    },
    "folha": {
      "$id": "#/properties/folha",
      "type": "integer",
      "title": "The Folha Schema",
      "default": 0,
      "examples": [
        1
      ]
    },
    "termo": {
      "$id": "#/properties/termo",
      "type": "integer",
      "title": "The Termo Schema",
      "default": 0,
      "examples": [
        3
      ]
    }
  }
}